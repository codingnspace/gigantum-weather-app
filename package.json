{
  "name": "commuter-app",
  "version": "1.0.0",
  "description": "Simple boilerplate demonstrating how to setup a project using React, Redux, Flow",
  "main": "scripts/start.js",
  "scripts": {
    "start": "node scripts/start",
    "dist": "rm -rf dist;node scripts/build;node scripts/prerender;",
    "build": "rm -rf dist;node --use_strict scripts/build;",
    "lint": "eslint src/js/**/*.js",
    "test": "jest"
  },
  "keywords": [
    "hot",
    "esri",
    "react",
    "redux",
    "Caddy",
    "https",
    "http2",
    "es6",
    "flux",
    "webpack",
    "hot-reload",
    "javascript",
    "esri-webpack",
    "hot-module-replacement"
  ],
  "author": "CButler",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "^js(.*)$": "<rootDir>/src/js$1",
      "alt.(js)$": "<rootDir>/node_modules/alt/dist/alt.js",
      "react-router.(js)$": "<rootDir>/node_modules/react-router/es/index.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/object.js",
      "\\.(jpe?g|png|gif|svg)$": "<rootDir>/__mocks__/object.js"
    }
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-form": "^7.1.1",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "alt": "^0.18.6",
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.3.1",
    "babel-plugin-transform-es2015-modules-amd": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^21.2.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "git+https://github.com/webpack/extract-text-webpack-plugin.git#v2.0.0-rc.0",
    "fetch-mock": "^5.13.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^19.0.2",
    "macaddress": "^0.2.8",
    "node-sass": "^3.13.1",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.3.0",
    "requirejs": "^2.3.2",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  }
}
